	local function EspPlaces_OFF()
		if workspace:FindFirstChild("ESPPlacesFolder") then
			workspace:FindFirstChild("ESPPlacesFolder"):Destroy()
		end
	end

	UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
		if not gameProcessedEvent then
			if input.KeyCode == Enum.KeyCode.K then
				if CanOpenBool == true then
					if OpenBool == true then
						OpenBool = false
						GUI_CLOSE()
						TweenService:Create(ColorCorrectionEffect, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
							Brightness = 0;
							Contrast = 0;
							Saturation = 0;
						}):Play()
						TweenService:Create(BlurEffect, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
							Size = 0;
						}):Play()
					else
						OpenBool = true
						GUI_OPEN()
						TweenService:Create(ColorCorrectionEffect, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
							Brightness = 0;
							Contrast = -0.25;
							Saturation = -0.6;
						}):Play()
						TweenService:Create(BlurEffect, TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
							Size = 24;
						}):Play()
					end
				end
			end
		end
	end)

