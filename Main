	-- Functions ::
	local function GUI_CLOSE()
		for i,v in pairs(ScreenGui:GetDescendants()) do
			if v.ClassName == "Frame" then
				if FrameTables[i] then
				else
					FrameTables[i] = {
						Child = v;
						FrameChildType = v.ClassName;
						OriginalPosition = v.Position;
						OriginalBackroundTransparent = v.BackgroundTransparency;
					}
				end
				TweenService:Create(
					v,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = 1;
						Position = v.Position - UDim2.new(0.014, 0, 0, 0);
					}
				):Play()
			elseif v.ClassName == "ImageButton" or v.ClassName == "ImageLabel" then
				if FrameTables[i] then
				else
					FrameTables[i] = {
						Child = v;
						FrameChildType = v.ClassName;
						OriginalPosition = v.Position;
						OriginalBackroundTransparent = v.BackgroundTransparency;
						OriginalImageTransparent = v.ImageTransparency;
					}
				end
				TweenService:Create(
					v,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = 1;
						ImageTransparency = 1;
						Position = v.Position - UDim2.new(0.014, 0, 0, 0);
					}
				):Play()
			elseif v.ClassName == "TextButton" or v.ClassName == "TextLabel" then
				if FrameTables[i] then
				else
					FrameTables[i] = {
						Child = v;
						FrameChildType = v.ClassName;
						OriginalPosition = v.Position;
						OriginalBackroundTransparent = v.BackgroundTransparency;
						OriginalTextTransparent = v.TextTransparency;
					}
				end
				TweenService:Create(
					v,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = 1;
						TextTransparency = 1;
						Position = v.Position - UDim2.new(0.014, 0, 0, 0);
					}
				):Play()
			elseif v.ClassName == "UIStroke" then
				if FrameTables[i] then
				else
					FrameTables[i] = {
						Child = v;
						FrameChildType = v.ClassName;
						OriginalTransparent = v.Transparency;
					}
				end
				TweenService:Create(
					v,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						Transparency = 1;
					}
				):Play()
			end
		end
	end

	local function GUI_OPEN()
		for i,v in pairs(FrameTables) do
			if v.FrameChildType == "Frame" then
				TweenService:Create(
					v.Child,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = v.OriginalBackroundTransparent;
						Position = v.OriginalPosition;
					}
				):Play()
			elseif v.FrameChildType == "ImageButton" or v.FrameChildType == "ImageLabel" then
				TweenService:Create(
					v.Child,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = v.OriginalBackroundTransparent;
						ImageTransparency = v.OriginalImageTransparent;
						Position = v.OriginalPosition;
					}
				):Play()
			elseif v.FrameChildType == "TextButton" or v.FrameChildType == "TextLabel" then
				TweenService:Create(
					v.Child,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						BackgroundTransparency = v.OriginalBackroundTransparent;
						TextTransparency = v.OriginalTextTransparent;
						Position = v.OriginalPosition;
					}
				):Play()
			elseif FrameTables[i].FrameChildType == "UIStroke" then
				local TRENSF = TweenService:Create(
					v.Child,
					TweenInfo.new(OpenCloseSecondsTween, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0),
					{
						Transparency = v.OriginalTransparent;
					}
				)
				TRENSF:Play()
			end
		end
	end

